{
  "name": "@dash-ui/stylis",
  "version": "1.0.0",
  "homepage": "https://github.com/dash-ui/stylis#readme",
  "repository": "github:dash-ui/stylis",
  "bugs": "https://github.com/dash-ui/stylis/issues",
  "license": "MIT",
  "description": "A fork of @emotion/stylis that makes it even smaller, still",
  "keywords": [
    "stylis"
  ],
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "source": "src/stylis.min.js",
  "types": "types/index.d.ts",
  "files": [
    "/dist",
    "/src",
    "/types"
  ],
  "exports": {
    ".": {
      "browser": "./dist/module/index.js",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/main/index.js",
      "source": "./src/index.ts",
      "types": "./types/index.d.ts",
      "default": "./dist/main/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "sideEffects": false,
  "scripts": {
    "build": "node scripts/build && npm run build-esm && npm run build-main && npm run build-module && npm run build-types && npm run format",
    "build-esm": "npm run compile -- -o dist/esm/index.mjs --env-name esm",
    "build-main": "npm run compile -- -o dist/main/index.js --env-name main",
    "build-module": "npm run compile -- -o dist/module/index.js --env-name module",
    "build-types": "cp -R node_modules/@emotion/stylis/types/* types",
    "compile": "babel src --ignore \"**/*.test.js\"",
    "format": "prettier --write \"**/*.{ts,js,md,yml,json,eslintrc,prettierrc}\"",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run format",
    "test": "jest",
    "validate": "npm run build && npm run lint && npm run test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,yml,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@emotion/stylis": "^0.8.5",
    "babel-jest": "^26.0.1",
    "babel-plugin-annotate-pure-calls": "latest",
    "dtslint": "^0.3.0",
    "eslint": "latest",
    "eslint-import-resolver-jest": "latest",
    "eslint-plugin-jest": "latest",
    "husky": "latest",
    "jest": "latest",
    "jscodeshift": "^0.5.0",
    "lint-staged": "latest",
    "prettier": "latest",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.8",
    "stylis": "^3.5.4",
    "stylis-rule-sheet": "^0.0.10"
  },
  "dependencies": {},
  "peerDependencies": {}
}
